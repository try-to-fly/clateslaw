# Grafana
GRAFANA_URL=https://your-grafana-instance.com
GRAFANA_TOKEN=your-grafana-api-token

# OpenClaw Message
OPENCLAW_CHANNEL=telegram
OPENCLAW_TARGET=your-chat-id

# Proxy (optional): required in some networks for OpenClaw Telegram sendMessage
HTTP_PROXY=http://127.0.0.1:7890
HTTPS_PROXY=http://127.0.0.1:7890

# AMAP key
VITE_AMAP_KEY=api-key

# AMP Web API key
AMP_WEB_API=api-key

# Park recommend push control
PARK_RECOMMEND_MIN_MOVE_METERS=1000
PARK_RECOMMEND_MIN_MS=1800000

# AMap around search
AMAP_AROUND_RADIUS=1500
AMAP_AROUND_TOPN=3
